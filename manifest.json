{
  "manifest_version": 3,
  "name": "Send Image To URL",
  "version": "1.0",
  "permissions": [
    "contextMenus",
    "activeTab",
    "storage",
    "unlimitedStorage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options.html",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
